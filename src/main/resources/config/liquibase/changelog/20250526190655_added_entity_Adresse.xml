<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity Adresse.
    -->
    <changeSet id="20250526190655-1" author="jhipster">
        <createTable tableName="adresse">
            <column name="idadresse" type="integer">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="novoie" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="btq" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="typvoie" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="codvoie" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="voie" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="codepostal" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="commune" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="coddep" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

</databaseChangeLog>
